{
  "name": "ng-admin-pro",
  "version": "12.2.0",
  "license": "SEE LICENSE IN <license.pdf>",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-scss": "cp -r projects/ng-uikit-pro-standard/src/lib/assets/ dist/ng-uikit-pro-standard",
    "copy-files": "cp projects/ng-uikit-pro-standard/src/lib/changelog projects/ng-uikit-pro-standard/src/lib/License.pdf dist/ng-uikit-pro-standard",
    "pack-archive": "cd dist/ng-uikit-pro-standard && npm pack . && cp ng-uikit-pro-standard-12.*.tgz ../..",
    "remove-archive": "rm dist/ng-uikit-pro-standard/ng-uikit-pro-standard-12.*.tgz && cd ../..",
    "compile": "ng-packagr -p projects/ng-uikit-pro-standard/ng-package.json && npm run copy-scss && npm run copy-files && npm run schematics:build && npm run pack-archive",
    "schematics:build": "tsc -p projects/ng-uikit-pro-standard/tsconfig.schematics.json && npm run schematics:copy-schema && npm run schematics:copy-collection && npm run schematics:copy-migration",
    "schematics:copy-schema": "cp projects/ng-uikit-pro-standard/schematics/ng-add/schema.json dist/ng-uikit-pro-standard/schematics/ng-add",
    "schematics:copy-collection": "cp projects/ng-uikit-pro-standard/schematics/collection.json dist/ng-uikit-pro-standard/schematics/",
    "schematics:copy-migration": "cp projects/ng-uikit-pro-standard/schematics/migration.json dist/ng-uikit-pro-standard/schematics/",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "3.0.0-beta.0",
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@types/googlemaps": "3.39.12",
    "angular-calendar": "^0.28.26",
    "chart.js": "^2.7.3",
    "core-js": "^2.5.4",
    "date-fns": "^2.23.0",
    "easy-pie-chart": "^2.1.7",
    "hammerjs": "^2.0.8",
    "ng-uikit-pro-standard": "file:ng-uikit-pro-standard-12.2.0.tgz",
    "rxjs": "~6.5.2",
    "rxjs-compat": "^6.3.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@angular/language-service": "~12.2.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^12.2.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.3.5"
  }
}
